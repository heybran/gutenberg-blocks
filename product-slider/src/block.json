{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "heybran/product-slider",
	"version": "0.1.0",
	"title": "Product Slider",
	"category": "widgets",
	"icon": "smiley",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"items": {
			"type": "array",
			"default": []
		},
		"thumbnailsPosition": {
			"type": "string",
			"default": "left"
		},
		"hasBorder": {
			"type": "boolean",
			"default": "true"
		}
	},
	"textdomain": "product-slider",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
